<p id="notice"><%= notice %></p>
<%= render 'layouts/header' %>

<h1>Listing Images</h1>
<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Title</th>
      <th>File</th>
      <th>Created at</th>
      <th>Address</th>
      <th>Comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @images.each do |image| %>
      <tr>
        <td><%= image.user.name %></td>
        <td><%= image.title %></td>
        <td><%= image_tag image.file %></td>
        <td><%= image.created_at %></td>
        <td><%= image.address %></td>
        <td><%= image.comment %></td>
        <td><%= link_to 'Show', image %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
